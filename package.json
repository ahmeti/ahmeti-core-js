{
  "name": "ahmeti-core-js",
  "version": "1.0.29",
  "description": "Ahmeti Core JS",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmeti/ahmeti-core-js"
  },
  "author": "Ahmet Imamoglu",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.2.0",
    "gulp": "^3.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-rename": "^1.4.0",
    "gulp-sequence": "^1.0.0",
    "laravel-mix": "^4.0.16"
  },
  "devDependencies": {
    "autonumeric": "^1.9.46",
    "bootstrap": "^3.4.0",
    "bootstrap-datepicker": "^1.8.0",
    "bootstrap-multiselect": "^0.9.13-1",
    "bootstrap-tabdrop-ro": "^1.0.0",
    "chart.js": "^2.8.0",
    "datatables.net-dt": "^1.10.19",
    "datatables.net-responsive-dt": "^2.2.3",
    "font-awesome": "^4.0.0",
    "fullcalendar": "^3.9.0",
    "history.js": "git+https://github.com/browserstate/history.js.git",
    "jquery": "^3.4.0",
    "jquery-form": "^4.2.2",
    "metismenu": "^2.7.9",
    "moment": "^2.23.0",
    "select2": "^4.0.6-rc.1",
    "sweetalert2": "^7.33.1",
    "tinyColorPicker": "git+https://github.com/PitPik/tinyColorPicker.git",
    "tinymce": "4.9.11"
  },
  "resolutions": {
    "graceful-fs": "4.2.3"
  }
